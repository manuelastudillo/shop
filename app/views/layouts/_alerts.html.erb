<div class="container-fluid ">
 <div class="row" style="padding-top: 55px;"">
  <% if notice.present? %>
   <div class="alert alert-info" role="alert"><%= notice %></div>
  <% end %>

  <% if alert.present? %>
   <div class="alert alert-danger" role="alert"><%= alert %></div>
  <% end %>
 </div>
</div>